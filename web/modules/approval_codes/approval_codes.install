<?php

/**
 * @file
 * Install, update and uninstall functions for the Approval codes module.
 */

/**
 * Implements hook_install().
 */
function approval_codes_install() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

/**
 * Implements hook_uninstall().
 */
function approval_codes_uninstall() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}
